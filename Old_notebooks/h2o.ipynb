      ">>> test[\"CAPSULE\"] = test[\"CAPSULE\"].asfactor()\n",
      "\n",
      "\n",
      ">>> # Build a (classification) GLM\n",
      ">>> from h2o.estimators import H2OGeneralizedLinearEstimator\n",
      ">>> prostate_glm = H2OGeneralizedLinearEstimator(family=\"binomial\", alpha=[0.5])\n",
